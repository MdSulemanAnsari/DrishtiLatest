                android:fontFamily="@font/overpass_bold"
                android:text="$100"
                android:textColor="@color/adamColor"
                android:textSize="@dimen/_11sdp"
                app:layout_constraintStart_toEndOf="@+id/imageShoppingStockVendor"
                app:layout_constraintTop_toBottomOf="@+id/tvShoppingEmeraldVendor" />
            <TextView
                android:id="@+id/plus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/overpass_bold"
                android:layout_marginStart="@dimen/_2sdp"
                android:text="+"
                android:textColor="@color/adamColor"
                android:textSize="@dimen/_11sdp"
                app:layout_constraintStart_toEndOf="@+id/tvShoppingDollarVendor"
                app:layout_constraintTop_toBottomOf="@+id/tvShoppingEmeraldVendor" />
            <TextView
                android:id="@+id/tvShoppingDollarVendorAdd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/overpass_bold"
                android:text="$30"
                android:layout_marginStart="@dimen/_2sdp"
                android:textColor="@color/adamColor"
                android:textSize="@dimen/_11sdp"
                app:layout_constraintStart_toEndOf="@+id/plus"
                app:layout_constraintTop_toBottomOf="@+id/tvShoppingEmeraldVendor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageShoppingStockVendor"
                android:layout_width="@dimen/_60sdp"
                android:layout_marginTop="@dimen/_4sdp"
                android:layout_marginBottom="@dimen/_4sdp"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/emerald_stock_pic"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/buttonDeleteVendor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16sdp"
                android:layout_marginEnd="@dimen/_10sdp"
